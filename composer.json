{
    "name": "assoconnect/phpenqueue-azure-service-bus",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/phpenqueue-azure-service-bus/"
        }
    ],
    "type": "library",
    "description": "Azure service bus Transport",
    "keywords": ["messaging", "queue", "azure", "storage"],
    "homepage": "https://github.com/assoconnect/",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "queue-interop/queue-interop": "^0.7|^0.8",
        "enqueue/enqueue": "^0.9.0@dev",
        "microsoft/windowsazure": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "enqueue/test": "0.9.x-dev",
        "enqueue/null": "0.9.x-dev",
        "queue-interop/queue-spec": "0.6.x-dev"
    },
    "support": {
        "email": "github@assoconnect.com",
        "issues": "https://github.com/assoconnect/enqueue-azure/issues",
        "source": "https://github.com/assoconnect/enqueue-azure/",
        "docs": "https://github.com/assoconnect/enqueue-azure/blob/master/docs/index.md"
    },
    "autoload": {
        "psr-4": { "Enqueue\\AzureServiceBus\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
